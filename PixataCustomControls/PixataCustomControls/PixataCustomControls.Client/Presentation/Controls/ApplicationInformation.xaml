<UserControl x:Class="PixataCustomControls.Presentation.Controls.ApplicationInformation"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vc="clr-namespace:PixataCustomControls.Presentation.Controls">
  <UserControl.Resources>
    <vc:ApplicationInformationIndexToInformationValueVC x:Key="InformationIndexToInformationValueVC" />
  </UserControl.Resources>
  <TextBlock Text="{Binding Properties[PixataCustomControls:ApplicationInformation/Information], Converter={StaticResource InformationIndexToInformationValueVC}}" />
</UserControl>
