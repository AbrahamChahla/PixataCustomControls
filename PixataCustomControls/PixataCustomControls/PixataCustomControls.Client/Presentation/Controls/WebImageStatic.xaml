<UserControl x:Class="PixataCustomControls.Presentation.Controls.WebImageStatic"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Image Source="{Binding Properties[PixataCustomControls:WebImageStatic/Uri]}"
         Width="{Binding Properties[PixataCustomControls:WebImageStatic/Width]}"
         Height="{Binding Properties[PixataCustomControls:WebImageStatic/Height]}" />
</UserControl>
