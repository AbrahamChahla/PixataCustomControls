<?xml version="1.0" encoding="utf-8" ?>
<ModelFragment
  xmlns="http://schemas.microsoft.com/LightSwitch/2010/xaml/model"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <Control Name="StaticToolbar"
           SupportedContentItemKind="Value"
           DesignerImageResource="PixataCustomControls.StaticToolbar::ControlImage">
    <Control.Attributes>
      <DisplayName Value="Pixata Static Toolbar" />
    </Control.Attributes>

    <Control.Properties>
      <!-- *** General *** -->
      <ControlProperty Name="ImageWidth"
                       PropertyType=":Int32"
                       CategoryName="Appearance"
                       EditorVisibility="PropertySheet">
        <ControlProperty.Attributes>
          <DisplayName Value="Image width" />
          <Description Value="Set the width of the image for each visible button (ignore if not using images)" />
        </ControlProperty.Attributes>
        <ControlProperty.DefaultValueSource>
          <ScreenExpressionTree>
            <ConstantExpression ResultType=":Int32"
                                Value="32"/>
          </ScreenExpressionTree>
        </ControlProperty.DefaultValueSource>
      </ControlProperty>
      <ControlProperty Name="ImageHeight"
                       PropertyType=":Int32"
                       CategoryName="Appearance"
                       EditorVisibility="PropertySheet">
        <ControlProperty.Attributes>
          <DisplayName Value="Image height" />
          <Description Value="Set the height of the image for each visible button (ignore if not using images)" />
        </ControlProperty.Attributes>
        <ControlProperty.DefaultValueSource>
          <ScreenExpressionTree>
            <ConstantExpression ResultType=":Int32"
                                Value="32"/>
          </ScreenExpressionTree>
        </ControlProperty.DefaultValueSource>
      </ControlProperty>
      <ControlProperty Name="Margin"
                       PropertyType=":String"
                       CategoryName="Appearance"
                       EditorVisibility="PropertySheet">
        <ControlProperty.Attributes>
          <DisplayName Value="Button margin" />
          <Description Value="The margin to be used for all buttons (format l,t,r,b)" />
        </ControlProperty.Attributes>
        <ControlProperty.DefaultValueSource>
          <ScreenExpressionTree>
            <ConstantExpression ResultType=":String"
                                Value="0,0,5,0"/>
          </ScreenExpressionTree>
        </ControlProperty.DefaultValueSource>
      </ControlProperty>

      <!-- *** Button 1 *** -->
      <ControlProperty Name="Visible1"
                       PropertyType=":Boolean"
                       CategoryName="Appearance"
                       EditorVisibility="PropertySheet">
        <ControlProperty.Attributes>
          <DisplayName Value="Show button 1?" />
          <Description Value="Set whether button 1 should be displayed or not" />
        </ControlProperty.Attributes>
        <ControlProperty.DefaultValueSource>
          <ScreenExpressionTree>
            <ConstantExpression ResultType=":Boolean"
                                Value="True"/>
          </ScreenExpressionTree>
        </ControlProperty.DefaultValueSource>
      </ControlProperty>
      <ControlProperty Name="Image1"
                       PropertyType=":String"
                       UIEditorId="ImageEditor"
                       CategoryName="Appearance"
                       EditorVisibility="PropertySheet">
        <ControlProperty.Attributes>
          <DisplayName Value="Image #1" />
          <Description Value="Image number one" />
        </ControlProperty.Attributes>
      </ControlProperty>
      <ControlProperty Name="Text1"
                       PropertyType=":String"
                       CategoryName="Appearance"
                       EditorVisibility="PropertySheet">
        <ControlProperty.Attributes>
          <DisplayName Value="Text #1" />
          <Description Value="Text for button number one" />
        </ControlProperty.Attributes>
      </ControlProperty>
      <ControlProperty Name="ToolTip1"
                       PropertyType=":String"
                       CategoryName="Appearance"
                       EditorVisibility="PropertySheet">
        <ControlProperty.Attributes>
          <DisplayName Value="Tool tip #1" />
          <Description Value="Text for the tool tip for button number one" />
        </ControlProperty.Attributes>
      </ControlProperty>
      <ControlProperty Name="TagText1"
                       PropertyType=":String"
                       CategoryName="Appearance"
                       EditorVisibility="PropertySheet">
        <ControlProperty.Attributes>
          <DisplayName Value="Tag text #1" />
          <Description Value="Text for the tag for button number 1 (gets sent in the event args when the button is clicked)" />
        </ControlProperty.Attributes>
      </ControlProperty>

      <!-- *** Button 2 *** -->
      <ControlProperty Name="Visible2"
                       PropertyType=":Boolean"
                       CategoryName="Appearance"
                       EditorVisibility="PropertySheet">
        <ControlProperty.Attributes>
          <DisplayName Value="Show button 2?" />
          <Description Value="Set whether button 2 should be displayed or not" />
        </ControlProperty.Attributes>
        <ControlProperty.DefaultValueSource>
          <ScreenExpressionTree>
