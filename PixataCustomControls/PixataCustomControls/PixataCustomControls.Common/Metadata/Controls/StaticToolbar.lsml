        </ControlProperty.DefaultValueSource>
      </ControlProperty>
      <ControlProperty Name="Image2"
                       PropertyType=":String"
                       UIEditorId="ImageEditor"
                       CategoryName="Appearance"
                       EditorVisibility="PropertySheet">
        <ControlProperty.Attributes>
          <DisplayName Value="Image #2" />
          <Description Value="Image number 2" />
        </ControlProperty.Attributes>
      </ControlProperty>
      <ControlProperty Name="Text2"
                       PropertyType=":String"
                       CategoryName="Appearance"
                       EditorVisibility="PropertySheet">
        <ControlProperty.Attributes>
          <DisplayName Value="Text #2" />
          <Description Value="Text for button number 2" />
        </ControlProperty.Attributes>
      </ControlProperty>
      <ControlProperty Name="ToolTip2"
                       PropertyType=":String"
                       CategoryName="Appearance"
                       EditorVisibility="PropertySheet">
        <ControlProperty.Attributes>
          <DisplayName Value="Tool tip #2" />
          <Description Value="Text for the tool tip for button number 2" />
        </ControlProperty.Attributes>
      </ControlProperty>
      <ControlProperty Name="TagText2"
                       PropertyType=":String"
                       CategoryName="Appearance"
                       EditorVisibility="PropertySheet">
        <ControlProperty.Attributes>
          <DisplayName Value="Tag text #2" />
