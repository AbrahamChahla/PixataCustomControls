                       EditorVisibility="PropertySheet">
        <ControlProperty.Attributes>
          <DisplayName Value="Tool tip #5" />
          <Description Value="Text for the tool tip for button number 5" />
        </ControlProperty.Attributes>
      </ControlProperty>
      <ControlProperty Name="TagText5"
