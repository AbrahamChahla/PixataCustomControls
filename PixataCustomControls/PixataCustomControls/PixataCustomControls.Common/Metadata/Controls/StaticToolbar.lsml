        <ControlProperty.DefaultValueSource>
          <ScreenExpressionTree>
            <ConstantExpression ResultType=":Boolean"
                                Value="True"/>
          </ScreenExpressionTree>
        </ControlProperty.DefaultValueSource>
      </ControlProperty>
      <ControlProperty Name="Image3"
                       PropertyType=":String"
                       UIEditorId="ImageEditor"
                       CategoryName="Appearance"
                       EditorVisibility="PropertySheet">
        <ControlProperty.Attributes>
          <DisplayName Value="Image #3" />
          <Description Value="Image number 3" />
        </ControlProperty.Attributes>
      </ControlProperty>
      <ControlProperty Name="Text3"
                       PropertyType=":String"
                       CategoryName="Appearance"
                       EditorVisibility="PropertySheet">
        <ControlProperty.Attributes>
          <DisplayName Value="Text #3" />
          <Description Value="Text for button number 3" />
        </ControlProperty.Attributes>
      </ControlProperty>
      <ControlProperty Name="ToolTip3"
                       PropertyType=":String"
                       CategoryName="Appearance"
                       EditorVisibility="PropertySheet">
        <ControlProperty.Attributes>
          <DisplayName Value="Tool tip #3" />
          <Description Value="Text for the tool tip for button number 3" />
        </ControlProperty.Attributes>
      </ControlProperty>
      <ControlProperty Name="TagText3"
                       PropertyType=":String"
                       CategoryName="Appearance"
                       EditorVisibility="PropertySheet">
        <ControlProperty.Attributes>
          <DisplayName Value="Tag text #3" />
          <Description Value="Text for the tag for button number 3 (gets sent in the event args when the button is clicked)" />
        </ControlProperty.Attributes>
      </ControlProperty>

      <!-- *** Button 4 *** -->
      <ControlProperty Name="Visible4"
                       PropertyType=":Boolean"
