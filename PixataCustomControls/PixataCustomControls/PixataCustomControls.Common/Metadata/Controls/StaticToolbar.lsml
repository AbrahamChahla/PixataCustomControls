                       EditorVisibility="PropertySheet">
        <ControlProperty.Attributes>
          <DisplayName Value="Text #4" />
          <Description Value="Text for button number 4" />
        </ControlProperty.Attributes>
      </ControlProperty>
      <ControlProperty Name="ToolTip4"
                       PropertyType=":String"
                       CategoryName="Appearance"
                       EditorVisibility="PropertySheet">
        <ControlProperty.Attributes>
          <DisplayName Value="Tool tip #4" />
          <Description Value="Text for the tool tip for button number 4" />
        </ControlProperty.Attributes>
      </ControlProperty>
      <ControlProperty Name="TagText4"
                       PropertyType=":String"
